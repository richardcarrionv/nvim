{
  "ThreePartTable": {
    "prefix": "threetable",
    "body": [
      "\\begin{singlespace}",
      "\t\\begin{ThreePartTable}",
      "\t\t\\footnotesize",
      "\t\t\t\\begin{longtable}{l l}",
      "\t\t\t\t\\caption{$1}",
      "\t\t\t\t\\label{table:$2} \\\\\\\\",
      "\t\t\t\t\\toprule",
      "\t\t\t\t\\multicolumn{1}{c}{\\textbf{$3}} & ",
      "\t\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
      "\t\t\t\t\\midrule",
      "\t\t\t\t\\endfirsthead",
      "\t\t\t\t\\toprule",
      "\t\t\t\t\\multicolumn{1}{c}{\\textbf{$3}} & ",
      "\t\t\t\t\\multicolumn{1}{c}{\\textbf{$4}} \\\\\\\\",
      "\t\t\t\t\\midrule",
      "\t\t\t\t\\endhead",
      "\t\t\t\t$5",
      "\t\t\t\t\\bottomrule",
      "\t\t\t\\end{longtable}",
      "\t\\end{ThreePartTable}",
      "\\end{singlespace}",
      "$0"
    ],
    "description": "Add a three part long table"
  },
  "historia": {
    "prefix": "historia",
    "body": [
      "\\historia{historiacaption = Historia de usuario - $1,",
      "\thistorialabel = {table:historia-$2},",
      "\tusuario = ${3},",
      "\tnombre =  $1,",
      "\tprioridad = ${4:Alta},",
      "\triesgo = ${5:Alta},",
      "\tpuntos = ${6:1},",
      "\titeracion = ${7:1},",
      "\tdescripcion = {$8},",
      "\tobservaciones = {$9},",
      "}"
    ],
    "description": "Historia de usuario"
  },

  "prettyref": {
    "prefix": "pref",
    "body": "\\prettyref{$1}",
    "description": "Pretty Reference"
  },
  "listing": {
    "prefix": "smlst",
    "body": [
      "\\begin{hyphen}",
      "\t\\lstinputlisting[caption={$1}, label={lst:$2}]{$3}",
      "\\end{hyphen}"
    ],
    "description": "Listado"
  },
  "listingLineRange": {
    "prefix": "lst",
    "body": [
      "\\lstinputlisting[caption={$1}, linerange={TXo-TXf}, label={lst:$2}]{$3}"
    ],
    "description": "Listado con linerange"
  }
}

